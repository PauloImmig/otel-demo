{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "OpenTelemetrySettings": {
    "AzureMonitor": {
      "ConnectionString": "YOUR_AZUREMONITOR_CONNECTIONSTRING"
    }
  },
  "OpenMeteoClient": {
    "BaseUri": "https://api.open-meteo.com/"
  }
}
